{
    "name": "graviton/common-bundle",
    "description": "Common functionality AND DEPENDENCIES for our php apps",
    "license": "MIT",
    "authors": [
        {
            "name": "List of contributors",
            "homepage": "https://github.com/libgraviton/DeploymentServiceBundle/graphs/contributors"
        }
    ],
    "require": {
        "composer-runtime-api": "^2",

        "symfony/framework-bundle": "^6.4.0",
        "symfony/security-bundle": "^6.4.0",
        "symfony/psr-http-message-bridge": "^6.4.0",
        "symfony/yaml": "^6.4.0",
        "symfony/stopwatch": "^6.4.0",
        "symfony/monolog-bundle": "~3.0",

        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "~7",
        "laminas/laminas-diactoros": "~3",
        "doctrine/mongodb-odm-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~10"
    },
    "autoload": {
        "psr-4": {
            "Graviton\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graviton\\CommonBundleTest\\": "tests/"
        }
    }
}

{
    "name": "graviton/common-bundle",
    "description": "Common functionality for our php apps",
    "license": "MIT",
    "authors": [
        {
            "name": "List of contributors",
            "homepage": "https://github.com/libgraviton/DeploymentServiceBundle/graphs/contributors"
        }
    ],
    "require": {
        "composer-runtime-api": "^2",
        "symfony/symfony": "~6",
        "auxmoney/opentracing-bundle-jaeger": "~1",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "~7",
        "laminas/laminas-diactoros": "~2",
        "doctrine/cache": "~1|~2",
        "doctrine/mongodb-odm": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "Graviton\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graviton\\CommonBundleTest\\": "tests/"
        }
    }
}
